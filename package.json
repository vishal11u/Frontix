{
  "name": "frontix",
  "version": "1.1.1",
  "description": "Modern utility toolkit for frontend developers â€” 25+ utilities for strings, dates, validation, async operations, deep objects, and more. TypeScript-first, tree-shakable, and lightweight. Includes debounce, throttle, deep cloning, date formatting, validation, and more.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "utility",
    "frontend",
    "typescript",
    "lodash-alternative",
    "string-utils",
    "date-utils",
    "array-utils",
    "number-utils",
    "debounce",
    "format-date",
    "validation",
    "async-utils",
    "deep-clone",
    "deep-merge",
    "case-conversion",
    "slugify",
    "phone-validation",
    "email-validation",
    "retry",
    "timeout",
    "throttle",
    "time-ago",
    "timezone",
    "date-comparison",
    "safe-value",
    "tree-shakable",
    "lightweight",
    "modern",
    "es6",
    "esm"
  ],
  "author": "Vishal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vishal11u/frontix.git"
  },
  "bugs": {
    "url": "https://github.com/vishal11u/frontix/issues"
  },
  "homepage": "https://github.com/vishal11u/frontix#readme",
  "dependencies": {
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
