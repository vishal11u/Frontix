{
  "name": "frontix",
  "version": "1.0.0",
  "description": "Modern utility toolkit for frontend developers â€” simple, lightweight, and powerful",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "clean": "rimraf dist"
  },
  "keywords": [
    "utility",
    "frontend",
    "typescript",
    "lodash-alternative",
    "string-utils",
    "date-utils",
    "array-utils",
    "number-utils",
    "debounce",
    "format-date"
  ],
  "author": "Vishal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vishal11u/frontix.git"
  },
  "bugs": {
    "url": "https://github.com/vishal11u/frontix/issues"
  },
  "homepage": "https://github.com/vishal11u/frontix#readme",
  "dependencies": {
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
